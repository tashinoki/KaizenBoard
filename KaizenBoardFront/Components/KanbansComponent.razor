@using Contract.Entity;
@using MudBlazor;

<MudButton OnClick="HandleClick">Button</MudButton>
@foreach (var kanban in Kanbans)
{
    <div>@kanban.Name</div>
}


@code {

    [Parameter]
    public IList<Kanban> Kanbans { get; set; } = new List<Kanban> { };

    public async override Task SetParametersAsync(ParameterView parameters)
    {
    }

    protected async override Task OnInitializedAsync()
    {
        Console.WriteLine("");
    }

    private async Task HandleClick()
    {
        Console.WriteLine("");
    }

}
